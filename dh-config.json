[
  {
    "name": "Docker Hub DNS A",
    "command": "dig",
    "arguments": [
      "hub.docker.com",
      "A"
    ]
  },
  {
    "name": "Docker Hub DNS AAAA",
    "command": "dig",
    "arguments": [
      "hub.docker.com",
      "AAAA"
    ]
  },
  {
    "name": "Docker Registry DNS A",
    "command": "dig",
    "arguments": [
      "registry-1.docker.io",
      "A"
    ]
  },
  {
    "name": "Docker Registry DNS AAAA",
    "command": "dig",
    "arguments": [
      "registry-1.docker.io",
      "AAAA"
    ]
  },
  {
    "name": "Docker Registry Auth DNS A",
    "command": "dig",
    "arguments": [
      "auth.docker.io",
      "A"
    ]
  },
  {
    "name": "Docker Registry Auth DNS AAAA",
    "command": "dig",
    "arguments": [
      "auth.docker.io",
      "AAAA"
    ]
  },
  {
