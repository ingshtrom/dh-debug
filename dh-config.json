    "name": "Cloudflare IPv4",
    "command": "curl",
    "arguments": [
      "-4svo", "/dev/null", "https://production.cloudflare.docker.com/"
    ]
  },
  {
    "name": "Cloudflare IPv6",
    "command": "curl",
    "arguments": [
      "-6svo",
