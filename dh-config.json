    ]
  },
  {
    "name": "Docker Registry IPv6",
    "command": "curl",
    "arguments": [
      "-6svo /dev/null https://registry-1.docker.io/"
    ]
  },
  {
    "name": "Docker Hub IPv4",
    "command": "curl",
    "arguments": [
      "-4svo /dev/null https://hub.docker.com/"
    ]
  },
  {
    "name": "Docker Hub IPv6",
    "command": "curl",
    "arguments": [
      "-6svo /dev/null https://hub.docker.com/"
    ]
  },
  {
    "name": "Docker Registry Auth IPv4",
    "command": "curl",
    "arguments": [
      "-4svo", "/dev/null", "https://auth.docker.io/"
    ]
  },
  {
    "name": "Docker Registry Auth IPv6",
    "command": "curl",
    "arguments": [
      "-6svo", "/dev/null", "https://auth.docker.io/"
